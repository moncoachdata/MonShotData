     "output_type": "stream",
     "text": [
      "import pysnooper\n",
      "\n",
      "@pysnooper.snoop()\n",
      "def add_sub(a, b):\n",
      "    \n",
      "    add = a+b\n",
      "    sub = a-b\n",
      "    \n",
      "    return (add, sub)\n",
      "    \n",
      "add_sub(18, 10)"
     ]
    }
   ],
