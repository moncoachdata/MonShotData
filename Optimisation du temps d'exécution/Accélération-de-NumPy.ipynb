   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "import numexpr as ne"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "33e1fbeb-1200-40e9-b193-8a30b1647f19",
   "metadata": {},
   "outputs": [],
   "source": [
    "a = np.random.random(10**7) \n",
    "b = np.random.random(10**7)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "90a0001d-3a67-4a0e-93d1-39693c577193",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "171 ms ± 7.09 ms per loop (mean ± std. dev. of 7 runs, 1 loop each)\n"
     ]
    }
   ],
   "source": [
    "%timeit np.cos(a) + np.sin(b)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "41a27e3c-46eb-45bb-9b37-dffe9bc66072",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "42.4 ms ± 2.33 ms per loop (mean ± std. dev. of 7 runs, 10 loops each)\n"
     ]
    }
   ],
   "source": [
    "%timeit ne.evaluate(\"cos(a) + sin(b)\")"
   ]
  }
